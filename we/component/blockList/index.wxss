.blocklist {
    width: 100%;
    height: 200rpx;
    background: #f4f2f2;
    border-radius: 4px;
    box-shadow: 0 0 10rpx 1rpx #f4f2f2;
    display: flex;
}

.bl-left {
    width: 40%;
    height: 100%;
    background: red;
    display: flex;
    flex-direction: column;
}

.bl-left-box {
    height: 50%;
    width: 100%;
}

.bl-right {
    width: 60%;
}